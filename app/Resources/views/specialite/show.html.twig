{% extends 'adminbase.html.twig' %}

{% block admin %}
    <h1>Specialite</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ specialite.id }}</td>
            </tr>
            <tr>
                <th>Nomspecialite</th>
                <td>{{ specialite.nomSpecialite }}</td>
            </tr>
            <tr>
                <th>Dateenreg</th>
                <td>{% if specialite.dateEnreg %}{{ specialite.dateEnreg|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('specialite_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('specialite_edit', { 'id': specialite.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
