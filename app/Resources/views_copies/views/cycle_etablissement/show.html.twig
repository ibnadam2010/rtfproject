            </tr>
        </tbody>
    </table>

    <ul>

            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        
        <ul>
            <a href="{{ path('cycle_etablissement_index') }}">Retour Ã  la liste</a>
        
