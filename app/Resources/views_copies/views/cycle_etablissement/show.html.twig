    <table class="table table-hover table-responsive">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cycle_etablissement.id }}</td>
            </tr>
            <tr>
                <th>Etablissement</th>
                <td>{{ cycle_etablissement.etablissement.nomEtablissement }}</td>
            </tr>
            <tr>
                <th>Cycle</th>
                <td>{{ cycle_etablissement.cycle.nomCycle }}</td>
            </tr>
            <tr>
